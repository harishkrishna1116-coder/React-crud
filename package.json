  "scripts": {
    "server": "json-server --watch db.json --port 4000",
    "test": "jest",
    "smells": "eslint src/app/**/*.{ts,tsx}",
    "analyze:smells": "npx reactsniffer2 apps/react-crud/src && node generate-report.js && start reports/react-sniffer-report.html",
    "duplication": "jscpd apps  --min-lines 5 --ignore \"apps/react-crud/src/app/Modules/Users.tsx\" --reporters console,html --output reports/jscpd && start \"\" \"reports\\jscpd\\html\\index.html\"",
    "codehealth": "rimraf reports/code-health && code-health-meter --srcDir \"apps/react-crud/src/app\" --outputDir \"reports/code-health\" --format html && start \"\" \"reports\\code-health\\code-complexity-audit\\CodeComplexityReport.html\" && start \"\" \"reports\\code-health\\code-modularity-audit\\CodeModularityReport.html\"",
    "complexity": "node generate-complexity-report.js && start \"\" \"reports\\complexity\\index.html\""
  },
